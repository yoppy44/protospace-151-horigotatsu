<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting"> 
        <%= "こんにちは、" %>
        <%= link_to current_user.name , user_path(@user.id), class:  :greeting__link %>
        <%= "さん" %>
      </div>
    <% end %>

    <div class="card__wrapper">
      <% @prototypes.each do |prototype| %>
        <%= render partial: "prototype", locals: { prototype: prototype } %>
      <% end %>  

    </div>
  </div>
</main>